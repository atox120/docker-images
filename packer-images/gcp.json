{
  "variables": {
      "gcp_account_file": "",
      "gcp_project_id": ""
    },
  "builders": [
    { 
      "type": "googlecompute",
      "account_file": "{{user `gcp_account_file`}}",
      "project_id": "{{user `gcp_project_id`}}",
      "source_image": "ubuntu-1604-xenial-v20190112",
      "ssh_username": "packer",
      "zone": "us-central1-a",
      "image_name": "packer-example-{{timestamp}}",
      "image_description": ""
    }
  ],
  "provisioners": [{
    "type": "shell",
    "inline": [
      "sleep 30",
      "sudo apt-get update",
      "sudo apt-get install -y redis-server"
    ]
  }]
}
